<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field" package="com.bonitasoft.engine.core.process.instance.model.impl">

    <filter-def name="tenantFilter" condition=":tenantId = tenantid">
        <filter-param name="tenantId" type="long" />
    </filter-def>

    <class name="com.bonitasoft.engine.core.process.instance.model.breakpoint.impl.SBreakpointImpl" table="breakpoint">
        <composite-id class="org.bonitasoft.engine.persistence.PersistentObjectId" mapped="true">
            <key-property name="tenantId" column="tenantid" type="long" />
            <key-property name="id" column="id" type="long" />
        </composite-id>
        <property name="stateId" column="state_id" />
        <property name="interruptedStateId" column="int_state_id" />
        <property name="elementName" column="elem_name" />
        <property name="isInstanceScope" column="inst_scope" />
        <property name="instanceId" column="inst_id" />
        <property name="definitionId" column="def_id" />
        <filter name="tenantFilter" />
    </class>



</hibernate-mapping>
