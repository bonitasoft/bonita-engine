msgid "%attribute% file format is not allowed. Only %file_formats% files are allowed."
msgstr "%attribute% ファイルの形式は許可されていません。%file_formats%のファイルのみが許可されます。"

msgid "%attribute% file format not allowed"
msgstr "%attribute%のファイル形式は許可されていません。"

msgid "%attribute% file format not allowed or not starting with correct servlet path"
msgstr "%attribute% ファイル形式は正しいサーブレットパスで開始できません。"

msgid "%attribute% is mandatory"
msgstr "%attribute%は必須です。"

msgid "%attribute% is not a valid Color value"
msgstr "%attribute% は、有効なカラー値ではありません。"

msgid "%attribute% is not a valid URL"
msgstr "%attribute% は、有効な URL ではありません。"

msgid "%attribute% is not a valid email"
msgstr "%attribute% は、有効なメール・アドレスではありません。"

msgid "%attribute% is not well written"
msgstr "%attribute% は、よく書かれていません。"

msgid "%attribute% must be a boolean value"
msgstr "%attribute% は、ブール値である必要があります。"

msgid "%attribute% must be a negative value"
msgstr "%attribute% は、マイナス値でなければなりません。"

msgid "%attribute% must be a negative value diffrent from 0"
msgstr "%attribute% は、ゼロ以下のマイナス値にする必要があります。"

msgid "%attribute% must be a numeric value"
msgstr "%attribute% は数値でなければなりません"

msgid "%attribute% must be a positive value"
msgstr "%attribute% は、正の値でなければなりません。"

msgid "%attribute% must be a positive value different from 0"
msgstr "%attribute% は、ゼロ以上の正数にする必要があります。"

msgid "%attribute% must be a valid date"
msgstr "%attribute% は有効な日付にする必要があります。"

msgid "%attribute% must be an integer value"
msgstr "%attribute% は、 整数値を指定する必要があります。"

msgid "%attribute% must be equal to %secondAttribute%"
msgstr "%attribute% は、 %secondAttribute% に等しくなければなりません。"

msgid "%attribute% must be less or equal than %value%"
msgstr "%attribute% は、%value%以下にする必要があります。"

msgid "%attribute% must be less than %secondAttribute%"
msgstr "%attribute% は、 %secondAttribute% より小さくなければなりません。"

msgid "%attribute% must be less than %value%"
msgstr "%attribute% は、%value%より小さくなければなりません"

msgid "%attribute% must be more or equal than %secondAttribute%"
msgstr "%attribute% は、%secondAttribute% 以上にする必要があります。"

msgid "%attribute% must be more or equal than %value%"
msgstr "%attribute% は、%value%以上にする必要があります。"

msgid "%attribute% must be more than %secondAttribute%"
msgstr "%attribute% は、%secondAttribute% より大きくなければなりません。"

msgid "%attribute% must be more than %value%"
msgstr "%attribute% は、%value%より大きくなければなりません"

msgid "%attribute% must be on a single line"
msgstr "%attribute% は、1 行にする必要があります。"

msgid "%attribute% must be one of {%list%}"
msgstr "%attribute% は、{%list%}のいずれかする必要があります。"

msgid "%attribute% must contain exactly %length% characters"
msgstr "%attribute% には、正確に%length%文字が含まれている必要があります。"

msgid "%attribute% must not be equal to %secondAttribute%"
msgstr "%attribute% は、 %secondAttribute% と異なっていなければなりません。"

