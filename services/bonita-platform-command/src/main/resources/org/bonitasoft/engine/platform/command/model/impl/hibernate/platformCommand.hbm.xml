<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field"
	package="org.bonitasoft.engine.platform.command.model.impl">

	<class name="SPlatformCommandImpl" table="platformCommand">
		<composite-id class="org.bonitasoft.engine.persistence.PersistentObjectId" mapped="true">
			<key-property name="id" column="ID" type="long" />
		</composite-id>
		<property name="name" type="java.lang.String" column="name" />
		<property name="description" type="java.lang.String" column="description" />
		<property name="implementation" type="java.lang.String" column="implementation" />
	</class>
</hibernate-mapping>
